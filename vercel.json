{
  "version": 2,
  "builds": [
    {
      "src": "api/run.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/clear_cache.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/run",
      "dest": "/api/run.py"
    },
    {
      "src": "/api/clear_cache",
      "dest": "/api/clear_cache.py"
    },
    {
      "src": "/api/debug",
      "dest": "/api/run.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}